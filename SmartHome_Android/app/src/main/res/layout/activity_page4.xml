<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEDEDE">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="56dp"
        android:id="@+id/imageView7"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:keepScreenOn="true"
        android:src="@drawable/list4_title"/>

    <ImageButton
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:id="@+id/imageButton2"
        android:src="@drawable/f5"
        android:adjustViewBounds="true"
        android:scaleType="fitStart"
        android:background="@android:color/transparent"
        android:layout_gravity="right"
        android:layout_x="321dp"
        android:layout_y="13dp"
        android:onClick="onButtonClicked" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_x="1dp"
        android:layout_y="48dp">


        <ToggleButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imageButton8"
            android:src="@drawable/list4_ok"
            android:adjustViewBounds="true"
            android:background="@drawable/list4_ok_grey"
            android:scaleType="fitStart"
            android:layout_marginTop="91dp"
            android:textOn= ""
            android:textOff=""
            android:layout_below="@+id/imageView17"
            android:layout_alignStart="@+id/imageView17"
            android:checked="false"
            android:onClick="onButton1Clicked" />

        <ToggleButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imageButton9"
            android:src="@drawable/list4_ok"
            android:adjustViewBounds="true"
            android:background="@drawable/list4_no_grey"
            android:scaleType="fitStart"
            android:layout_marginStart="17dp"
            android:textOn= ""
            android:textOff=""
            android:layout_alignTop="@+id/imageButton8"
            android:layout_toEndOf="@+id/imageButton8"
            android:checked="false"
            android:onClick="onButton2Clicked"/>

        <ImageView
            android:layout_width="280dp"
            android:layout_height="45dp"
            android:id="@+id/imageView17"
            android:adjustViewBounds="true"
            android:background="@android:color/transparent"
            android:scaleType="fitStart"
            android:src="@drawable/list4_text1"
            android:layout_marginStart="21dp"
            android:layout_marginTop="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:id="@+id/imageView28"
            android:src="@drawable/list4_text2"
            android:layout_below="@+id/imageButton8"
            android:layout_alignEnd="@+id/imageButton8"
            android:layout_marginEnd="12dp" />

        <ImageView
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:id="@+id/imageView29"
            android:src="@drawable/list4_text3"
            android:layout_marginEnd="12dp"
            android:layout_below="@+id/imageButton9"
            android:layout_alignEnd="@+id/imageButton9" />
    </RelativeLayout>

</AbsoluteLayout>